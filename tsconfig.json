{
    "compilerOptions": {
        "resolveJsonModule": true,
        "inlineSourceMap": true,
        "rootDir": "./",
        "removeComments": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "downlevelIteration": true,
    }
}